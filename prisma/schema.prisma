generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("TRIP_DATABASE_URL")
}

model Admin {
  id        Int @id @default(autoincrement())
  username  String  @db.VarChar(255)
  password  String  @db.VarChar(255)
}

model User {
  id        Int @id @default(autoincrement())
  username  String  @db.VarChar(255)
  password  String  @db.VarChar(255)
}
